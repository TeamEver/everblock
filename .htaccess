# Apache 2.2
<IfModule !mod_authz_core.c>
    <FilesMatch "(\.php|\.log|\.sql|\.xlsx)$">
        Order allow,deny
        Deny from all
    </FilesMatch>
</IfModule>

# Apache 2.4
<IfModule mod_authz_core.c>
    <FilesMatch "(\.php|\.sql|\.xlsx)$">
        Require all denied
    </FilesMatch>
</IfModule>
